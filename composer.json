{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "alperenersoy/filament-export": "^3.0",
        "artesaos/seotools": "^1.3",
        "blade-ui-kit/blade-heroicons": "^2.5",
        "filament/filament": "3.2",
        "filament/notifications": "3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "ladumor/laravel-pwa": "^0.0.4",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.1",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.5",
        "mckenziearts/laravel-notify": "^2.5",
        "mindscms/laravelshoppingcart": "^2.1",
        "minishlink/web-push": "^9.0",
        "morilog/jalali": "^3.4",
        "notifylk/notify-php": "*@dev",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-sitemap": "^7.2",
        "stripe/stripe-php": "^14.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "fly-apps/dockerfile-laravel": "^1.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.34",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "refresh": [
            "@php artisan db:wipe",
            "@php artisan migrate",
            "@php artisan db:seed"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/notifylk/notify-php.git"
        }
    ]
}
